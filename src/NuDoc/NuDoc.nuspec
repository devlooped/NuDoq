<package>
    <metadata>
        <id>NuDoc</id>
        <version>1.0</version>
        <title>NuDoc: A .NET XML API Documentation Reader</title>
        <authors>Daniel Cazzulino, kzu, Clarius Labs</authors>
        <owners>Clarius Labs</owners>
        <licenseUrl>https://github.com/clariuslabs/common/blob/master/License.txt</licenseUrl>
