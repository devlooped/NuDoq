<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>NuDoq</PackageId>
    <Description>A lightweight API to read and write .NET XML documentation files, optionally augmented with reflection information.</Description>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="ThisAssembly" Version="2.0.12" PrivateAssets="all" />
    <PackageReference Include="NuGetizer" Version="1.2.4" PrivateAssets="all" />
  </ItemGroup>

</Project>
